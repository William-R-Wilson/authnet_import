Credit Card Report for: <%= @data["TxnDate"] %>

<div>
  <table>
    <tr>
      <th>Employee</th>
      <th>Description</th>
      <th>Amount</th>
      <th>Account</th>
      <th>Class</th>
    </tr>
  <% @data["Line"].each do |l| %>
      <tr>
        <td><%= %></td>
        <td><%= l["Description"] %></td>
        <td><%= l["Amount"] %></td>
        <td><%= l["AccountBasedExpenseLineDetail"]["AccountRef"]["name"] %></td>
        <td><%= l["AccountBasedExpenseLineDetail"]["ClassRef"]["name"] %></td>
      </tr>
  <% end %>
      <tr>
        <td></td>
        <td></td>
        <td><strong>Total: </strong><%= @data["TotalAmt"] %></td>
        <td></td>
        <td></td>
  </table>
</div>

<div>
  <form method="post" action="send_cc_to_quickbooks">
    <input type="submit" value="Send to Quickbooks">
  </form>
</div>
